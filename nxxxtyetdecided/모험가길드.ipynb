   "source": [
    "n = int(input())\n",
    "horror = list(map(int, input().split()))\n",
    "\n",
    "# 오름차순 정렬\n",
    "horror.sort()\n",
    "# 사람 수, 그룹 수\n",
    "p , g = 1, 0\n",
    "\n",
    "\n",
    "for i in horror:\n",
    "    # 사람 수가 공포감보다 크다면\n",
    "    if p >= i:\n",
    "        g += 1\n",
    "        p = 1\n",
    "    else:\n",
    "        p += 1\n",
    "\n",
    "print(g)\n"
