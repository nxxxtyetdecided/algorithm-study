   ],
   "source": [
    "# 학생 수 입력 받기\n",
    "n = int(input())\n",
    "\n",
    "arr = []\n",
    "for _ in range(n):\n",
    "    arr.append(input().split()) # ()를 하나로 인식하여 삽입됨\n",
    "\n",
    "arr = sorted(arr, key=lambda x:x[1])\n",
    "\n",
    "for i in arr:\n",
    "    print(i[0], end=\" \")"
   ],
